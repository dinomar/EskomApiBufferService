{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "Serilog": {
      "LogLevel": {
        "Default": "Information"
      },
      "PathFormat": "Logs/log-{Date}.txt"
    }
  },
  "AllowedHosts": "*",
  "EskomBufferService": {
    "DelayInMinutes": 60,
    "Retries": 3,
    "StatusMinRange": 0,
    "StatusMaxRange": 10,
    "MaxLogs": 1000
  }
}
